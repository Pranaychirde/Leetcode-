import java.util.Scanner;

public class CheckPrime {
    public static void main(String[] args) {
        System.out.print("Enter a number :");
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        boolean result = true;

        for(int i = 2;i<=n/2;i++){
            if(n%i == 0){
                result = false;
            }
        }
        System.out.println("Checking Prime : "+result);

    }
}
